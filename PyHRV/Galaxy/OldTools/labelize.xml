<tool id="labelize" name="Labelize" version="1.0.0">
  <description>tool</description>

  <command interpreter="python">
    labelize_w.py -i $input1 -o $out_file1 -w $w -j $j
  </command>

  <inputs>
      <param name="input1" type="data" format="tgz"  />
      <param name="w" label="Window Length [s]" type="text" value="40" />
      <param name="j" label="Moving interval in seconds for centering new windows set (0=each beat)" type="text" value="0" />
  </inputs>
  <outputs>
    <data name="out_file1" format="tgz"  />
  </outputs>

  <help>

  </help>
</tool>
